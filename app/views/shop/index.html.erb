<div class="cbox_large float_l">
	<h2>Produkty</h2>
	<table>
	<% @products.each do |product| %>
	  <tr>
	  <td><%= product.name %></td>
	  <td><%= link_to "More", product %></td>
	  </tr>
	<% end %>
	</table>
</div>
<div class="cbox_small float_r">
	<h2>Kategorie</h2>
	<table>
	<% @categories.each do |category| %>
	  <tr>
	  <td><%= category.name %></td>
	  <td><%= link_to "More", category %></td>
	  </tr>
	<% end %>
	</table>
</div>